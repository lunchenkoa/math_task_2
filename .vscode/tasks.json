{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: g++-11 build active file",
			"command": "/usr/bin/g++-11",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"${file}", "functions.cpp", "iof.cpp",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}",
				"-I", "/usr/local/include/eigen-3.4.0/"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/g++-11"
		}
	]
}